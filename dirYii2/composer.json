{
  "name": "initYii",
  "description": "initYii",
  "type": "project",
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "^2.0.28",
    "wodrow/yii-fast-api": "^0.1.0",
    "wodrow/yii2wajaxcrud": "dev-master",
    "wodrow/yii2wtxtcrawler": "^0.1.14",
    "wodrow/yii2-wwtree": "dev-master",
    "kartik-v/yii2-widget-fileinput": "^1.0.9",
    "kartik-v/yii2-field-range": "^1.3.5",
    "wodrow/yii2-scheduling": "dev-master",
    "yii2mod/yii2-array-query": "^1.4",
    "geoip2/geoip2": "^2.9.0",
    "ralouphie/mimey": "^2.1.0",
    "mdmsoft/yii2-admin": "~2.0",
    "dmstr/yii2-adminlte-asset": "^2.6.2",
    "yiisoft/yii2-swiftmailer": "^2.1"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.1.11",
    "yiisoft/yii2-gii": "^2.1.2"
  },
  "config": {
    "process-timeout": 300,
    "preferred-install": "dist",
    "fxp-asset": {
      "installer-paths": {
        "asset-pattern-skip-version": "(-build|-patch)",
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}
