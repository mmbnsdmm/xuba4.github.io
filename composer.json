{
  "name": "ysxcapi",
  "description": "ysxcapi",
  "require": {
    "php": ">=7.1",
    "yiisoft/yii2": "^2.0",
    "wodrow/yii-fast-api": "^0.1",
    "wodrow/yii2wajaxcrud": "^3.0",
    "wodrow/yii2wtxtcrawler": "^0.1",
    "kartik-v/yii2-widget-fileinput": "^1.0",
    "kartik-v/yii2-field-range": "^1.3",
    "kartik-v/yii2-widget-select2": "^2.1",
    "kartik-v/yii2-icons": "^1.4",
    "omnilight/yii2-scheduling": "^1.1",
    "league/flysystem-sftp": "^1.0",
    "creocoder/yii2-flysystem": "^0.9",
    "guzzlehttp/guzzle": "^6.3",
    "yii2mod/yii2-array-query": "^1.4",
    "anchnet/aliyun-openapi-php-sdk": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "^2.1",
    "yiisoft/yii2-gii": "^2.1"
  },
  "config": {
    "process-timeout": 300,
    "preferred-install": "dist",
    "fxp-asset": {
      "installer-paths": {
        "asset-pattern-skip-version": "(-build|-patch)",
        "npm-asset-library": "vendor/npm",
        "bower-asset-library": "vendor/bower"
      }
    }
  }
}
